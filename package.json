    "walletlink": "^2.0.2",
    "web3": "^1.2.6"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "react-app-rewired start",
    "build": "CI=false && rimraf ./build && react-app-rewired --max-old-space-size=3096 build && rimraf ./build/**/*.map",
    "test": "CI=true && react-app-rewired test --coverage",
    "ci": "CI=false && react-app-rewired --max-old-space-size=3096 build",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss,less}'",
    "lint": "eslint 'src/**/*.{js,jsx,html}' --fix --ignore-pattern 'src/lightcone/oracle/bandchain/BandChainCopy.js' --ignore-pattern 'src/lightcone/oracle/bandchain/Obi.js'"
  },
  "lint-staged": {
    "**/*.{js, html}": [
      "eslint 'src/**/*.{js,jsx,html}' --fix --ignore-pattern 'src/lightcone/oracle/bandchain/BandChainCopy.js' --ignore-pattern 'src/lightcone/oracle/bandchain/Obi.js'",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@types/jest": "^26.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-unused-imports": "^0.1.2",
    "express-async-handler": "^1.1.4",
    "git-revision-webpack-plugin": "^3.0.4",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "mocha": "^8.0.1",
    "prettier": "^2.0.2",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react-app-rewired": "^2.1.5",
    "react-dev-utils": "^10.1.0",
    "react-hot-loader": "^4.12.18",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.2.2"
  }
}
